datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Test {
  id                      Int       @id @default(autoincrement())
  createdAt               DateTime  @default(now()) @map(name: "created_at")
  center                  String
  test                    Json
  test_key                String    @unique @default(uuid())
  // email                 String    @unique
  // nombre                String
  // apellidos             String
  // documento_identidad   String    @unique
}